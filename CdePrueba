//CASO//
with
    var a <- true,b <- false,c : bool
    var x, y : int
    var z <- 'z' : char
    var m : array [3] of int
begin
    if a>b ->
        a <- b + c;
    otherwise ->
        a <- c;
    end
end
//END//

//CASO//
with
    var a <- true,b <- false,c : bool
    var x, y : int
    var z <- 'z' : char
    var m : array [3] of int
begin
    a<-a::b;
    for i from 1 to 4 ->
        a[0]<-a;
    end
end
//END//